#include "GPSSetup.h"
#include "Globals.h"

SFE_UBLOX_GNSS gps;

///////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
// ------------------------------- SETUP --------------------------------------------------------------------------------------------//
///////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////


void setup() {
  Serial.begin(115200);

  Serial.println("Initializing GPS");
  
  if(reading_GPS) {
    if(!setupGPS(gps)) {
      Serial.println(F("u-blox GNSS not detected at default I2C address. Please check wiring."));
    }
  }

}
///////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
// ------------------------------- LOOP ---------------------------------------------------------------------------------------------//
///////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////

void loop() {
  
  if (millis() - lastTime > 100) {
      readGPS(gps);
  }
  


//  delay(100);

}
